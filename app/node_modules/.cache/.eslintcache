"182","messages":"183","suppressedMessages":"184","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"185","messages":"186","suppressedMessages":"187","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/home/reis/Documentos/programacao/react/metal-spotify/app/src/index.tsx",[],[],["188","189"],"/home/reis/Documentos/programacao/react/metal-spotify/app/src/App.tsx",["190","191","192","193"],[],"/home/reis/Documentos/programacao/react/metal-spotify/app/src/store/index.ts",["194"],[],"import { createStore } from 'redux'\nimport rootReducer from './rootReducer'\n\nimport { applyMiddleware } from '@reduxjs/toolkit'\nimport { SagaMiddleware } from 'redux-saga'\nimport createSagaMiddleware from 'redux-saga'\nimport { persistStore, persistReducer } from 'redux-persist'\nimport storage from 'redux-persist/lib/storage'\n\n\n\nimport rootSaga from './rootSaga'\n\nconst sagaMiddleware = createSagaMiddleware()\nconst persistConfig = {\n    key: 'root',\n    storage\n}\n\nconst persistedReducer = persistReducer(persistConfig, rootReducer)\n\nconst store = createStore(persistedReducer,\n   applyMiddleware(sagaMiddleware))\n\nconst persistor = persistStore(store)\nsagaMiddleware.run(rootSaga)\n\nexport {store, persistor}\n","/home/reis/Documentos/programacao/react/metal-spotify/app/src/store/rootReducer.ts",[],[],"/home/reis/Documentos/programacao/react/metal-spotify/app/src/styles/global.ts",[],[],"/home/reis/Documentos/programacao/react/metal-spotify/app/src/routes/index.tsx",["195","196","197"],[],"import React, { useEffect } from 'react'\n\nimport {Navigate, useNavigate, Route, Routes, BrowserRouter} from 'react-router-dom'\n\n\nimport Home from '../pages/Home'\n\nimport  Album from '../pages/Album'\n\nimport UserHome from '../pages/UserHome'\n\nimport { useLocation } from 'react-router-dom'\n\nimport { useDispatch } from 'react-redux'\nimport { OutAlbumPage, InAlbumPage } from '../store/spotify/action'\nexport default function MyRoutes(){\n\n\n    const dispatch = useDispatch()\n    const location = useLocation()\n\n\n    useEffect(()=>{\n\n        const [, page,] = location.pathname.split('/')\n        if(page === 'album'){\n          dispatch(\n              InAlbumPage()\n          )\n        }else{\n            dispatch(\n            OutAlbumPage()\n            )\n        }\n\n    }, [location, dispatch])\n\n\n    return (\n        \n            <Routes>\n\n\n\n            {localStorage.getItem(\"token_token\") ?\n            (\n                <Route\n                element={\n                    <UserHome />\n                }\n                path='/'\n                />\n            ) :\n            <Route\n            element={\n                <Home />\n            }\n            path='/'\n            />\n            }\n\n            <Route\n            element={\n                <Album />\n            }\n            path='/album/:id'\n            />  \n           \n\n            </Routes>\n        \n     \n    )\n}\n","/home/reis/Documentos/programacao/react/metal-spotify/app/src/pages/Home/index.tsx",["198","199"],[],"import { HomeContainer } from './styles'\nimport Cover from '../../images/Cover.png'\nimport { ListenButton } from '../../components/ListenButton'\nimport { RollDiv } from '../../components/RollDiv'\nimport { Card } from '../../components/Card'\n\nimport { useDispatch } from 'react-redux'\n\nimport { SignIn } from '../../store/auth/action'\nexport default function Home(){\n\n    const dispatch = useDispatch()\n\n\n\n    function handleSignIn(){\n    dispatch(\n        SignIn()\n    )\n\n\n}\n\n\n    const array = [0, 1, 2, 3, 5, 6, 7, 8, 9, 10, 11, 12]\n\n    return (\n        <HomeContainer>\n        <h2>30 anos de Vulgar Display of Power:</h2>\n        <p> 59 minutos de pura revolta, como um bom metal deve ser.</p>\n       \n        <div className='panteraDiv'>\n            <div className='imageDiv'>  \n                <img src={Cover} alt=\"\" />\n\n                \n            </div>\n\n            <div className='textDiv'>\n                <h3>\n                    Vulgar Display of Power\n                </h3>\n                <p>\n                    Pantera\n                </p>\n\n                <ListenButton text=\"Ouça agora!\" />\n            </div>\n        </div>\n\n\n        <h2>Albuns mais pesados que o próprio metal: </h2>\n\n        <RollDiv>\n            {array.map(x=>(\n                <Card link={'' }title={'Vulgar Display of Power'} description={'Pantera'}>\n                     <img src={Cover} />\n                </Card>\n            ))}\n           \n        </RollDiv>\n\n\n\n        </HomeContainer>\n    )\n}","/home/reis/Documentos/programacao/react/metal-spotify/app/src/components/Sidebar/index.tsx",[],[],"/home/reis/Documentos/programacao/react/metal-spotify/app/src/components/Sidebar/styles.ts",[],[],"/home/reis/Documentos/programacao/react/metal-spotify/app/src/components/Header/index.tsx",["200"],[],"import { useEffect, useState } from \"react\"\n\nimport { HeaderContainer, OptionsDiv} from \"./styles\"\n\nimport { useDispatch, useSelector } from \"react-redux\"\nimport { SignIn, GetToken, SignOut} from \"../../store/auth/action\"\nimport { GoButton } from \"../GoButton\"\n\nimport DownIcon from '../../images/Down.svg'\nimport UpIcon from '../../images/Up.svg'\n\nimport { useNavigate } from \"react-router-dom\"\n\ninterface Iusername {\n    user: {\n        username: string;\n    }\n}\n\nexport function Header(){\n\n    const dispatch = useDispatch()\n\n\n    const navigate = useNavigate()\n\n    const username = useSelector<Iusername>((state:Iusername)=> state.user?.username)\n\n    const [openOptionDiv, setOpenOptionDiv] = useState<boolean>(false)\n    const goOn = useSelector<boolean>((state:any)=>state.auth?.goOn)\n\n    useEffect(()=>{\n\n        if(goOn){\n            dispatch(\n                GetToken()\n            )\n        }\n                \n \n    }, [goOn, dispatch])\n     function handleSignIn(){\n    \n\n    \n    dispatch(\n        SignIn()\n    )\n\n    window.location.href= 'http://localhost:8080/auth/login'\n\n    }\n\n    function handleSignOut(){\n\n        dispatch(\n            SignOut()\n        )\n        \n        window.open('http://localhost:8080/auth/signout', '_blank')\n\n        window.location.reload()\n    }\n\n    return (\n        <>\n        <HeaderContainer>\n\n            { username && localStorage.getItem(\"token_token\") ? \n             <GoButton text={username}\n             onClick={()=>setOpenOptionDiv(!openOptionDiv)}\n             > \n             <img src={ openOptionDiv ? UpIcon : DownIcon} alt=\"\" />\n             </GoButton>\n             :\n             <GoButton onClick={handleSignIn} text={'Entrar'} />\n         }\n         \n         {openOptionDiv && (\n                <OptionsDiv>\n                <p onClick={()=>handleSignOut()}> Sair </p>     \n                </OptionsDiv>\n         )}\n      \n        </HeaderContainer>\n        </>\n    )\n}","/home/reis/Documentos/programacao/react/metal-spotify/app/src/components/Header/styles.ts",[],[],"/home/reis/Documentos/programacao/react/metal-spotify/app/src/components/GoButton/index.tsx",["201"],[],"import { Button } from \"./styles\";\nimport UserIcon from '../../images/UserIcon.svg'\n\ninterface IGoButton {\n    text: string | any;\n    onClick?:()=>any;\n    children?: React.ReactElement | React.ReactElement[]\n}\n\nexport function GoButton({text='', children, onClick}: IGoButton){\n\n\n    return (\n        <Button onClick={onClick}>\n            <img src={UserIcon} />\n            <p>{text}</p>\n            {children && children}\n        </Button>\n    )\n}","/home/reis/Documentos/programacao/react/metal-spotify/app/src/components/GoButton/styles.ts",[],[],"/home/reis/Documentos/programacao/react/metal-spotify/app/src/pages/Home/styles.ts",[],[],"/home/reis/Documentos/programacao/react/metal-spotify/app/src/components/ListenButton/index.tsx",["202","203"],[],"import { Button } from \"./styles\";\nimport UserIcon from '../../images/UserIcon.svg'\n\nimport PlayIcon from '../../images/PlayIcon.svg'\n\ninterface IGoButton {\n    text: string;\n}\n\nexport function ListenButton({text}: IGoButton){\n\n\n    return (\n        <Button>\n            <img src={PlayIcon} />\n            <span>{text}</span>\n        </Button>\n    )\n}","/home/reis/Documentos/programacao/react/metal-spotify/app/src/components/ListenButton/styles.ts",[],[],"/home/reis/Documentos/programacao/react/metal-spotify/app/src/components/RollDiv/index.tsx",[],[],"/home/reis/Documentos/programacao/react/metal-spotify/app/src/components/RollDiv/styles.ts",[],[],"/home/reis/Documentos/programacao/react/metal-spotify/app/src/components/Card/index.tsx",["204"],[],"import { CardContainer, PlayerDiv } from \"./styles\"\nimport PlayIcon from '../../images/PlayIcon.svg'\n\nimport {Link } from 'react-router-dom'\n\ninterface ICard {\n\n    children: React.ReactElement;\n    title: string;\n    description: string;\n    link:string;\n}\nexport function Card({\n    children,\n    title,\n    description,\n    link\n}: ICard){\n   \n    return (\n            <CardContainer>\n\n            {children} \n            <h3> {title} </h3>\n            <p> {description} </p>\n\n            <PlayerDiv> \n             <Link to={link}><img  className=\"play\" src={PlayIcon }/ ></Link>  \n            </PlayerDiv>\n\n            </CardContainer>\n    )\n}","/home/reis/Documentos/programacao/react/metal-spotify/app/src/components/Card/styles.ts",[],[],"/home/reis/Documentos/programacao/react/metal-spotify/app/src/pages/UserHome/index.tsx",["205","206","207","208","209","210","211","212"],[],"/home/reis/Documentos/programacao/react/metal-spotify/app/src/pages/UserHome/styles.ts",[],[],"/home/reis/Documentos/programacao/react/metal-spotify/app/src/store/auth/reducer.ts",[],[],"/home/reis/Documentos/programacao/react/metal-spotify/app/src/store/rootSaga.ts",[],[],"/home/reis/Documentos/programacao/react/metal-spotify/app/src/store/auth/saga.ts",["213","214","215"],[],"import { all, put, takeEvery, call, take} from 'redux-saga/effects'\nimport { push } from 'react-router-redux'\n\nimport { api }from '../../api'\n\nimport axios from 'axios'\n\ninterface IResponse {\n    data: {\n        access_token:string;\n    }\n}\nfunction* signIn(){\n\n   yield console.log('aaaaaaa')\n   yield window.location.href= 'http://localhost:8080/auth/login'\n\n   yield put({type: 'auth/SIGN_IN'})\n\n   \n}\n\n\nexport function* signOut(){\n\n    // yield call(\n    //     api.get,\n    //     '/auth/signout'\n    // )\n\n    yield put({\n        type: \"auth/SIGN_OUT\"\n    })\n}\n\nfunction* getToken(){\n    const response:IResponse = yield call(\n        api.post, \n        '/auth/token'\n    )\n\n    const token = `Bearer ${response.data}`\n        \n    yield put({type: \"auth/GET_TOKEN\", payload: token})\n\n    yield put({type: \"@user/GET_USER_INFO\", payload: { token }})\n    // const config = {\n    //     headers: {Authorization : token}\n    // }\n    // const responseUser: IResponse = yield call(\n    //     axios.get,\n    //     'https://api.spotify.com/v1/me',\n    //     config\n    // )\n    // console.log(responseUser)\n}\n\n\n\nexport default function* authSaga(){\n    yield all([\n        takeEvery('@auth/SIGN_IN', signIn),\n        takeEvery('@auth/GET_TOKEN', getToken),\n        takeEvery('@auth/SIGN_OUT', signOut)\n    ])\n}","/home/reis/Documentos/programacao/react/metal-spotify/app/src/api/index.ts",[],[],"/home/reis/Documentos/programacao/react/metal-spotify/app/src/store/auth/action.ts",[],[],"/home/reis/Documentos/programacao/react/metal-spotify/app/src/store/user/reducer.ts",[],[],"/home/reis/Documentos/programacao/react/metal-spotify/app/src/store/user/saga.ts",[],[],"/home/reis/Documentos/programacao/react/metal-spotify/app/src/store/spotify/reducer.ts",["216"],[],"import { AnyAction } from \"@reduxjs/toolkit\";\nimport { routerActions } from \"react-router-redux\";\n\ninterface IinitialState {\n   albuns: [];\n   artists:[];\n   albumPage: boolean;\n}\nlet initialState: IinitialState = {\n    albuns: [],\n    artists: [],\n    albumPage: false,\n} \n\nexport function spotifyReducer(state=initialState, action: AnyAction){\n\n    switch(action.type){\n\n        case \"spotify/GET_ALBUNS\":\n            return {...state,\n            albuns: action.payload.albuns}\n\n        case \"spotify/GET_ARTISTS\":\n            return {...state,\n                artists: action.payload.artists\n            }\n\n        case \"spotify/IN_ALBUM_PAGE\":\n            return {...state,\n            albumPage: true}\n        case \"spotify/OUT_ALBUM_PAGE\":\n            return {...state,\n            albumPage:false}\n        default:\n            return state;\n    }\n\n}","/home/reis/Documentos/programacao/react/metal-spotify/app/src/store/spotify/saga.ts",["217"],[],"import { AnyAction } from '@reduxjs/toolkit'\nimport { all, put, takeEvery, call, take} from 'redux-saga/effects'\n\nimport { spotifyApi } from '../../api'\n\n\ninterface Iimages {\n    url: string;\n}\ninterface Itracks {\n    album: {\n        name:string;\n        href:string;\n        images: Iimages[];\n        id: string;\n        artists: [{\n            name:string;\n        }]\n    }\n\n    artists: [{\n        id:string\n        name:string;\n    }]\n}\ninterface IResponse {\n    data: {\n       tracks: Itracks[]\n    }\n}\n\ninterface IResponseArtist {\n    data: {\n        name: string;\n        href:string;\n        images: Iimages[];\n        genres: string[];\n        id:string;\n    }\n}\n\n\n  function* getAlbuns(action: AnyAction){\n\n    const { token } = action.payload;\n\n    const config = {\n        headers: {Authorization: token }\n    }\n    const response: IResponse = yield call(spotifyApi.get,\n        '/recommendations?seed_genres=metal',\n        config\n        )\n\n      const albuns = response.data.tracks.map((x:Itracks)=>{\n          return {\n              name: x.album.name,\n              images: x.album.images,\n              id: x.album.id,\n              link: x.album.href,\n              artist: x.album.artists[0].name\n\n          }\n      })  \n\n\n\n\n    yield put({type: \"@spotify/GET_ARTISTS\", payload: {data: response.data.tracks, config} })\n    yield put({type: \"spotify/GET_ALBUNS\", payload: { albuns}})\n\n\n}\n\n\nfunction* getArtists(action: AnyAction){\n\n    const { data, config} = action.payload\n\n\n    let artists: any = [];\n    for(let i = 0; i<6; i++){\n\n        \n       let res: IResponseArtist = yield call(spotifyApi.get,\n            `/artists/${data[i].artists[0].id}`,\n            config)\n            artists = [...artists, {\n                name: res.data.name,\n                link: res.data.href,\n                images: res.data.images,\n                genres: res.data.genres,\n                id: res.data.id\n            }]\n    }\n\n       \n        \n    artists = artists.reverse()\n        console.log(artists)\n        yield put({type: \"spotify/GET_ARTISTS\", payload: {artists} })\n\n}\n\n\nexport default function* spotifySaga(){\n    yield all([\n        takeEvery(\"@spotify/GET_ALBUNS\", getAlbuns),\n        takeEvery(\"@spotify/GET_ARTISTS\", getArtists)       \n    ])\n}\n","/home/reis/Documentos/programacao/react/metal-spotify/app/src/store/spotify/action.ts",[],[],"/home/reis/Documentos/programacao/react/metal-spotify/app/src/pages/Album/index.tsx",["218","219","220","221","222","223","224","225"],[],"/home/reis/Documentos/programacao/react/metal-spotify/app/src/pages/Album/styles.ts",[],[],"/home/reis/Documentos/programacao/react/metal-spotify/app/src/components/SkeletonComponent/index.tsx",[],[],{"ruleId":"226","replacedBy":"227"},{"ruleId":"228","replacedBy":"229"},{"ruleId":"230","severity":1,"message":"231","line":7,"column":8,"nodeType":"232","me